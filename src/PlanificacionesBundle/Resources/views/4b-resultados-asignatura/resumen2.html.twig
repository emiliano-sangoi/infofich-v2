<div>

    {#{ dump(resumen.resultados)}#}

    <div class="row">        
        <div class="col-sm-10">
            {% if resumen.resultados[0] is not defined %}
                <p class="text-muted">                    
                    Sin definir.
                </p>
            {% else %}
                <ul>
                    {% for r in resumen.resultados %}
                        <li>
                            <p>                        
                                {{ r.resultado }}
                            </p>
                        </li>
                    {% endfor %}
                </ul>
            {% endif %}              
        </div>       
    </div>

    <div class="mt-4">          
        <a class="btn btn-outline-primary" href="{{ path('planif_resultados_editar', {id : planificacion.id}) }}">
            <i class="fa fa-edit"></i>&nbsp;Modificar
        </a>                    
    </div>

</div>